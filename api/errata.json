      { "operation": "updateApplication", "to": "application" },
      { "operation": "generateCsrForApplication", "to": "metadata" },
      { "operation": "grantConsentToScope", "to": "oAuth2ScopeConsentGrant" }
    ]
  },
  {
    "type": "renameApi",
    "parameters": [
      { "from": "ClientApi", "to": "Client" }
    ]
  },
  {
    "type": "removeParameter",
    "parameters": ["after", "limit"]
  },
  {
    "type": "renameParameter",
    "parameters": [
      { "from":  "oauthClient", "to":  "oAuthClient" },
      { "from":  "oauthTokens", "to":  "oAuthTokens" }
    ]
  },
  {
    "type": "overridePropertyType",
    "parameters": [
      { "format": "password", "type": "char[]" }
    ]
  }
